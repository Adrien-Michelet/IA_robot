<?xml version="1.0"?>
<listeObjectif>

	<objectif nom="Attente du GO" points="0" temp="5">
        <action methode="attendreDepart"/>
    </objectif>
    <objectif nom="Posseder un panneau" points="5" temp="0">
    </objectif>

    <objectif nom="Charger cube 1" points="0" temp="4">
        <conditions>
            <and>
                <variable nom="goldenCube1" condition="notZero"/>
                <variable nom="tourCubes" condition="notMax"/>
            </and>
        </conditions>
        <action methode="homologationAddGoldenCube1">
        </action>
    </objectif>

    <objectif nom="Charger cube 2" points="0" temp="4">
        <conditions>
            <and>
                <variable nom="goldenCube2" condition="notZero"/>
                <variable nom="tourCubes" condition="notMax"/>
            </and>
        </conditions>
        <action methode="homologationAddGoldenCube2">
        </action>
    </objectif>

    <objectif nom="Deposer Cube" points="3" temp="10">
        <conditions>
            <and>
                <variable nom="goldenCube1" condition="=" value="0"/>
                <variable nom="goldenCube2" condition="=" value="0"/>
                <variable nom="tourCubes" condition="=" value="2"/>
            </and>
        </conditions>
        <action methode="seDeplacerVersUnElement">
            <param nom="type" type="string" value="zoneConstruction"/>
            <param nom="vitesse" type="float" value="1"/>
        </action>
        <action methode="seDeplacerVersUnElement">
            <param nom="type" type="string" value="zoneConstructionMilieu"/>
            <param nom="vitesse" type="float" value="1"/>
        </action>
        <action methode="releaseCubes" />
    </objectif>
</listeObjectif>
