                    <param nom="distance" type="int" value="150"/>
                    <param nom="vitesse" type="float" value="0.4"/>
                </action>
                <action methode="avancer">
                    <param nom="distance" type="int" value="550"/>
                    <param nom="vitesse" type="float" value="0.4"/>
                </action>
            </onError>
        </action><action methode="seDeplacerXY" couleur="blue">
